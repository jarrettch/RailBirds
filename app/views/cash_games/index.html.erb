<h4>Cash Game Results</h4>
<br />
<table class="table table-hover">
<tr>
  <th>Date</th>
  <th>Location</th>
  <th>Game</th>
  <th>Buyin</th>
  <th>Cash Out</th>
  <th>Profit/Loss</th>
  <th>Notes</th>
  <th></th>
  <th></th>
</tr>
<% @cash_games.each do |game| %>
<tr>
  <td><%= game.end_session.strftime('%m/%d/%Y') %></td>
  <td><%= game.location %></td>
  <td><%= game.game %></td>
  <td>$<%= game.buyin_and_rebuys %>0</td>
  <td>$<%= game.cash_out %>0</td>
  <td>$<%= game.cash_out - game.buyin_and_rebuys %>0</td>
  <td><%= game.notes %></td>
  <td><%= link_to 'Edit', edit_cash_game_path(game) %></td>
  <td><%= link_to 'Delete', game, method: :delete, data: { confirm: 'Are you sure you want to delete this session?' } %></td>
</tr>
<% end %>
</table>
